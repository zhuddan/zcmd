{
  "publisher": "zdecode",
  "name": "zcmd",
  "displayName": "zcmd",
  "version": "0.0.14",
  "description": "My commonly used command line",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/zcmd.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "zcmd.create-taro-page",
        "title": "Create new taro page"
      },
      {
        "command": "zcmd.type-def",
        "title": "Create a typedef in JSDOC"
      },
      {
        "command": "zcmd.create-api",
        "title": "Create a api"
      },
      {
        "command": "zcmd.create-nest-js-module",
        "title": "Create a NestJS module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "zcmd.create-taro-page",
          "group": "2_navigate"
        },
        {
          "command": "zcmd.create-nest-js-module",
          "group": "2_navigate"
        }
      ],
      "editor/context": [
        {
          "command": "zcmd.type-def",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --fix",
    "test": "vscode-test",
    "tag": "bumpp",
    "publish": "vsce publish"
  },
  "dependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@types/ejs": "^3.1.5",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "bumpp": "^9.10.1",
    "ejs": "^3.1.10",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
